<div class="mContainer">
<h3> <%= @election.name %> </h3>

 <div class="lg-map-wrapper">
   <div id="lg-map"></div>
   <div class="lg-map-text"></div>
 </div>


<% @election.candidates_sorted_by_delegate_count.each do |c| %>
   <p><%= c.first_name %> <%= c.last_name %> - <%= c.delegate_count %></p>
<% end %>

<table>
   <tr>
      <th> State </th>
      <th> Total # Delegates </th>
      <% @election.candidates.each do |c| %>
         <th><%= c.full_name %></th>
      <% end %>
   </tr>
   
      <% @election.contests.each do |contest| %>
         <tr>
            <th><%= contest.state.name %></th>
            <th><%= contest.number_delegates %></th>
            <% @election.candidates.each do |c| %>
               <th> <%= contest.results.pledged.by_candidate(c.id).total_delegate_count %> </th>
            <% end %>
       </tr>
      <% end %>
   
</table>
</div>

<script>

</script>
